{
  "@jupyterlab/apputils-extension:themes": {
    "theme": "JupyterLab Dark"
  },
  "@jupyterlab/fileeditor-extension:plugin": {
    "editorConfig": {
      "wordWrapColumn": 90,
      "tabSize": 4,
      "insertSpaces": true,
      "matchBrackets": true,
      "autoClosingBrackets": true,
      "rulers": [90]
    }
  },
  "@jupyterlab/notebook-extension:tracker": {
    "codeCellConfig": {
      "autoClosingBrackets": true,
      "lineNumbers": true,
      "matchBrackets": true,
      "insertSpaces": true,
      "tabSize": 4,
      "wordWrapColumn": 90,
      "rulers": [90]
    },
    "markdownCellConfig": {
      "autoClosingBrackets": true,
      "lineNumbers": true,
      "matchBrackets": true,
      "insertSpaces": true,
      "tabSize": 4,
      "wordWrapColumn": 90,
      "rulers": [90]
    },
    "rawCellConfig": {
      "autoClosingBrackets": true,
      "lineNumbers": true,
      "matchBrackets": true,
      "insertSpaces": true,
      "tabSize": 4,
      "wordWrapColumn": 90,
      "rulers": [90]
    }
  },
  "@jupyterlab/filebrowser-extension:browser": {
    "showHiddenFiles": true
  },
  "@jupyterlab/codemirror-extension:commands": {
    "theme": "solarized dark"
  }
}
